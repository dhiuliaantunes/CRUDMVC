@model IEnumerable<OurWebSite.Models.Date>

@{
    ViewBag.Title = "Dates";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateNome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChuvoso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaEnsolarado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateNome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaChuvoso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaEnsolarado)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
